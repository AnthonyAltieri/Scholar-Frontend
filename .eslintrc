{
  "extends": "airbnb",
  "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/prop-types": [0, {}],
      "indent": [1, 2, { "SwitchCase": 1 }],
      "no-extra-boolean-cast": 0,
      "arrow-parens": [2, "as-needed", { "requireForBlockBody": true }]
  }
}
